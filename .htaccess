

Options -MultiViews
<IfModule mod_rewrite.c>
RewriteEngine on 			

DirectoryIndex _welcome.php
#DirectoryIndex home.php


# RewriteRule ^index.php$                                                                                     _welcome.php [QSA,L]
RewriteRule ^welcome/{0,1}$                                                                                     _welcome.php [QSA,L]
RewriteRule ^welcome/([a-zA-Z0-9_-]+)$                                                                                  _welcome.php?id1=$1 [QSA,L]


RewriteRule ^admin/{0,1}$						admin/_welcome.php
RewriteRule ^admin/login/{0,1}$                                         admin/login/_welcome.php
RewriteRule ^admin/index.php$                                           admin/_welcome.php
RewriteRule ^admin/login/index.php$                                     admin/login/_welcome.php

RewriteRule ^profileinfo   			        modules/registration/profile_info.php
