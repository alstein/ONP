RewriteRule  ^friend/([a-zA-Z0-9_-]+)/view_all_merchants_fan/page/([a-zA-Z0-9_-]+)/{0,1}  modules/friend/view_all_merchants_fan.php?id1=$1&page=$2
RewriteRule  ^friend/view_all_merchants_fan/page/([a-zA-Z0-9_-]+)/{0,1}  modules/friend/view_all_merchants_fan.php?page=$1

RewriteRule  ^deal/viewalldeal/page/([a-zA-Z0-9_-]+)/{0,1}  modules/deal/viewalldeal.php?page=$1
RewriteRule  ^merchant-account/view_search_merchant/page/([a-zA-Z0-9_-]+)/{0,1}  modules/merchant-account/view_search_merchant.php?page=$1

RewriteRule  ^deal/deal-notifications/page/([a-zA-Z0-9_-]+)/{0,1}  modules/deal/deal-notifications.php?page=$1


RewriteRule ^show_cheered_users/([a-zA-Z0-9_-]+)/{0,1}$	                modules/photos/show_cheered_users.php?imageid=$1
