RewriteRule ^message/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/view_message/{0,1}$                                             modules/message/view_message.php?msg_id=$1&status=$2


#RewriteRule ^friend/([a-zA-Z0-9_-]+)/([^/.]+)/view_all_search/{0,1}$                     modules/friend/view_all_search.php?id1=$1&id2=$2&id3=$3

RewriteRule ^my-account/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/my_profile_home/{0,1}$                     modules/my-account/my_profile_home.php?friendid=$1&id1=$2

# buy deal
RewriteRule ^buy/([a-zA-Z0-9_-]+)/{0,1}$     	modules/deal/viewdeal.php?deal_id=$1

RewriteRule ^([a-zA-Z0-9_-]+)/{0,1}$                                                                        modules/$1/$1.php
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/{0,1}$                                                       modules/$1/$2.php
RewriteRule ^([a-zA-Z0-9_-]+)/([^/.]+)/([a-zA-Z0-9_-]+)/{0,1}$                                              modules/$1/$3.php?id1=$2
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/{0,1}$                     modules/$1/$4.php?id1=$2&id2=$3
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/{0,1}$    modules/$1/$5.php?id1=$2&id2=$3&id3=$4

#  Module htaccess end
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule .+ - [L]

	# Page not found
	ErrorDocument 400 http://localoffersnpals.com/
	ErrorDocument 401 http://localoffersnpals.com/
	ErrorDocument 403 http://localoffersnpals.com/
	ErrorDocument 404 http://localoffersnpals.com/
	ErrorDocument 500 http://localoffersnpals.com/

