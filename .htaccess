RewriteRule ^([a-zA-Z0-9_-]+)/albumphotosinDetails/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/{0,1}$          modules/photos/ajaximageslider.php?user=$1&album=$2&imageid=$3 [QSA,L]

