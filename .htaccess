#-----------------------Photos-----------------------------------------------#
RewriteRule ^([a-zA-Z0-9_-]+)/create-album/{0,1}$				modules/photos/create-album.php?user=$1 [QSA,L]
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/photos/{0,1}$			modules/photos/photos.php?user=$1&album=$2 [QSA,L]
#RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/photos/{0,1}$			modules/photos/photos.php?user=$1&album=$2 [QSA,L]
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/photos/{0,1}$               	modules/photos/photos.php?user=$1&album=$2&page=$3 [QSA,L]
